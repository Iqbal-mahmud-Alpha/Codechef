#include <bits/stdc++.h>
using namespace std;
#define faster                    \
    ios_base::sync_with_stdio(0); \
    cin.tie(0);                   \
    cout.tie(0);
#define ff first
#define ss second
#define pb push_back
#define gcd(a, b) __gcd(a, b)
#define lcm(a, b) (a * (b / gcd(a, b)))
#define all(a) a.begin(), a.end()
#define mod 10000007
//#define num 1000000

/**----data type----*/
typedef long long int ll;
typedef unsigned long long int llu;

int main(){
    int tc;
    cin>>tc;
    while(tc--){
        string s;
        cin>>s;
        map<char,int>m;
        for(int i=0;i<3;i++){
            m[s[i]]++;
        }
        if(m.size()==3){
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
    }
}
