#include <bits/stdc++.h>
using namespace std;
#define faster  ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define ff first
#define ss second
#define pb push_back
#define gcd(a, b) __gcd(a, b)
#define lcm(a, b) (a * (b / gcd(a, b)))
#define all(a) a.begin(), a.end()
#define mod 10000007
#define num 1000000

/**----data type----*/
typedef long long int ll;
typedef unsigned long long int llu;

int main()
{
    int n,h,x,a;
    cin>>n>>h>>x;
    int flag=0;
    for(int i=0; i<n; i++)
    {
        cin>>a;
        int sum=a+x;
        if(sum==h)
        {
            flag=1;
            break;
        }
    }
    cout<<((flag==1)?"YES":"NO")<<endl;
}

